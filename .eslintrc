{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/no-unresolved": 0
    }
}
